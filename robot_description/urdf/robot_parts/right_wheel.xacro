        </inertial>
        <collision name="right_wheel_collision">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://robot_description/models/laser_robot/wheel/meshes/roda.obj" scale="1 1 1" />
            </geometry>
        </collision>
        <visual name="right_wheel_visual">
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://robot_description/models/laser_robot/wheel/meshes/roda.obj" scale="1 1 1" />
            </geometry>
        </visual>
